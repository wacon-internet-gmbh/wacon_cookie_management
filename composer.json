{
    "name": "waconcookiemanagement/wacon-cookie-management",
    "type": "typo3-cms-extension",
    "description": "Verschiedene Cookies k\u00f6nnen individuell zugelassen werden. Unterscheidung in notwendige und optionale Cookies",
    "authors": [
        {
            "name": "Kerstin Schmitt",
            "role": "Developer"
        }
    ],
    "require": {
        "typo3/cms-core": ">=9.5.20 <9.5.99 || >=10.4.2 <10.4.99"
    },
    "autoload": {
        "psr-4": {
            "Waconcookiemanagement\\WaconCookieManagement\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Waconcookiemanagement\\WaconCookieManagement\\Tests\\": "Tests"
        }
    },
    "replace": {
        "typo3-ter/wacon-cookie-management": "self.version"
    }
}
