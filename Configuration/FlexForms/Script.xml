<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <sheets>
        <sDEF>
            <ROOT>
               
                    <sheetTitle>
                        LLL:EXT:wacon_cookie_management/Resources/Private/Language/Backend.xlf:settings.registration.title
                    </sheetTitle>
                
                <type>array</type>
                <el>
                    <settings.cookie>
                        
                            <label>
                                LLL:EXT:wacon_cookie_management/Resources/Private/Language/Backend.xlf:settings.script.cookie
                            </label>
                            <config>
                                <type>select</type>
                                <renderType>selectSingle</renderType>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0"></numIndex>
                                        <numIndex index="1"></numIndex>
                                    </numIndex>
                                </items>
                                <foreign_table>tx_waconcookiemanagement_domain_model_cookie</foreign_table>
                                <foreign_table_where>
                                    AND kategorie > 0
                                </foreign_table_where>
                            </config>
                       
                    </settings.cookie>
                    <settings.script>
                       
                            <label>
                                LLL:EXT:wacon_cookie_management/Resources/Private/Language/Backend.xlf:settings.script.script
                            </label>
                            <config>
                                <type>text</type>
                                <cols>24</cols>
                                <rows>8</rows>
                            </config>
                       
                    </settings.script>
                    <settings.cookiecontent>
                        
                            <label>
                                LLL:EXT:wacon_cookie_management/Resources/Private/Language/Backend.xlf:settings.script.cookiecontent
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tt_content</allowed>
                                <rows>8</rows>
                            </config>
                       
                    </settings.cookiecontent>
                    <settings.text>
                        
                            <label>
                                LLL:EXT:wacon_cookie_management/Resources/Private/Language/Backend.xlf:settings.script.text
                            </label>
                            <config>
                                <type>input</type>
                            </config>
                        
                    </settings.text>
                    <settings.bild>
                        
                            <label>
                                LLL:EXT:wacon_cookie_management/Resources/Private/Language/Backend.xlf:settings.script.bild
                            </label>
                            <config>
                                <type>inline</type>
                                <foreign_table>sys_file_reference</foreign_table>
                                <foreign_field>uid_foreign</foreign_field>
                                <foreign_sortby>sorting_foreign</foreign_sortby>
                                <foreign_table_field>tablenames</foreign_table_field>
                                <foreign_match_fields>
                                    <!-- this will be stored in sys_file_reference.fieldname -->
                                    <fieldname>image</fieldname>
                                </foreign_match_fields>
                                <foreign_label>uid_local</foreign_label>
                                <foreign_selector>uid_local</foreign_selector>
                                <overrideChildTca>
                                    <columns>
                                        <uid_local>
                                            <config>
                                                <appearance>
                                                    <elementBrowserType>file</elementBrowserType>
                                                    <elementBrowserAllowed></elementBrowserAllowed>
                                                </appearance>
                                            </config>
                                        </uid_local>
                                    </columns>
                                </overrideChildTca>
                                <filter>
                                    <userFunc>TYPO3\CMS\Core\Resource\Filter\FileExtensionFilter->filterInlineChildren
                                    </userFunc>
                                    <parameters>
                                        <allowedFileExtensions></allowedFileExtensions>
                                        <disallowedFileExtensions></disallowedFileExtensions>
                                    </parameters>
                                </filter>
                                <appearance>
                                    <useSortable>1</useSortable>
                                    <headerThumbnail>
                                        <field>uid_local</field>
                                        <width>45</width>
                                        <height>45c</height>
                                    </headerThumbnail>
                                    <enabledControls>
                                        <info>1</info>
                                        <new>0</new>
                                        <dragdrop>1</dragdrop>
                                        <sort>0</sort>
                                        <hide>1</hide>
                                        <delete>1</delete>
                                    </enabledControls>
                                </appearance>
                            </config>
                        
                    </settings.bild>
                    <settings.nocookiecontent>
                        
                            <label>
                                LLL:EXT:wacon_cookie_management/Resources/Private/Language/Backend.xlf:settings.script.nocookiecontent
                            </label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>tt_content</allowed>
                                <rows>8</rows>
                            </config>
                        
                    </settings.nocookiecontent>
                </el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
