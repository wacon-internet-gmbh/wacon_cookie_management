    persistence {
        storagePid = {$module.tx_waconcookiemanagement_report.persistence.storagePid}
    }
    view {
        templateRootPaths.0 = EXT:wacon_cookie_management/Resources/Private/Backend/Templates/
        partialRootPaths.0 = EXT:wacon_cookie_management/Resources/Private/Backend/Partials/
        layoutRootPaths.0 = EXT:wacon_cookie_management/Resources/Private/Backend/Layouts/
